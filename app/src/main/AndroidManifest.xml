<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.accountmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConfirmNewCustomerInfo"></activity>
        <activity android:name=".NewCustomerAddress" />
        <activity android:name=".PurchaseAccessories" />
        <activity android:name=".NewCustomerMenu" />
        <activity android:name=".CreateNewAccount" />
        <activity android:name=".SalesMainPage" />
        <activity android:name=".FirstTimeLogin" />
        <activity android:name=".SignUp" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>